{
  "name": "sendit",
  "version": "1.0.0",
  "description": "SendIT is a courier service that helps users deliver parcels to different destinations. SendIT provides courier quotes based on weight catergories.",
  "main": "index.js",
  "scripts": {
    "heroku": "npm run build && node dist/index.js",
    "start": "nodemon --exec babel-node server/index.js",
    "build": "babel server/ -d dist",
    "host": "npm run build && node dist/index.js",
    "test": " babel-node server/models/migrate.js && NODE_ENV='test' nyc mocha --require @babel/register ./server/test/*.js --exit --5000",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "migration": "babel-node -- ./server/models/migrate"
  },
  "engines": {
    "node": "10.8.0",
    "npm": "6.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kellswork/SendIT.git"
  },
  "keywords": [
    "node"
  ],
  "author": "Kelechi Ogbonna",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Kellswork/SendIT/issues"
  },
  "homepage": "https://github.com/Kellswork/SendIT#readme",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bcrypt": "^3.0.2",
    "body-parser": "^1.18.3",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "express-promise-router": "^3.0.3",
    "joi": "^14.0.6",
    "jsonwebtoken": "^8.4.0",
    "pg": "^7.6.1",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/register": "^7.0.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.6",
    "nyc": "^13.1.0",
    "supertest": "^3.3.0"
  }
}
